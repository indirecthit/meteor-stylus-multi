#stylus-tests
  zoom: 1

dashy = dashed

.stylus-dashy-left-border
  border-left 1px dashy black

.stylus-nib-ellipsis
  overflow ellipsis

.stylus-autoprefixed-transition
  column-count 2

.stylus-jeet-column
  lost-column 1/2

.stylus-rupture-media-query
  color red

  +above(1px)
    color blue

.stylus-axis-border
  b()

.stylus-test-import-only
  color green

@import 'stylus-multi_test_import'
